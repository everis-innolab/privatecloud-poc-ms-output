{
	"kind": "List",
	"apiVersion": "v1",
	"metadata": {
		"name": "output-full"
	},
	"items": [{
		"kind": "Service",
		"apiVersion": "v1",
		"metadata": {
			"name": "ms-output-service",
			"labels": {
				"name": "ms-output-service"
			}
		},
		"spec": {
			"selector": {
				"name": "output-pod"
			},
			"ports": [{
				"protocol": "TCP",
				"port": 9992,
				"targetPort": 9992
			}]
		}
	},{
		"kind": "DeploymentConfig",
		"apiVersion": "v1",
		"metadata": {
			"name": "output-dc",
			"labels": {
				"name": "output-dc"
			}
		},
		"spec": {
			"strategy": {
				"type": "Recreate",
				"resources": {}
			},
			"triggers": [{
				"type": "ConfigChange"
			}],
			"replicas": 1,
			"selector": {
				"name": "output-pod"
			},
			"template": {
				"metadata": {
					"creationTimestamp": null,
					"labels": {
						"name": "output-pod"
					}
				},
				"spec": {
					"containers": [{
						"name": "output-container",
						"image": "output:latest",
						"ports": [{
							"name": "http",
							"containerPort": 9992,
							"protocol": "TCP"
						}],
						"terminationMessagePath": "/dev/termination-log",
						"imagePullPolicy": "IfNotPresent",
						"capabilities": {},
						"securityContext": {
							"capabilities": {},
							"privileged": false
						},
						"livenessProbe": {
							"tcpSocket": {
								"port": 9992
							},
							"timeoutSeconds": 1,
							"initialDelaySeconds": 120
						}
					}],
					"restartPolicy": "Always",
					"dnsPolicy": "ClusterFirst",
					"serviceAccount": "",
					"nodeSelector": {
						"region": "primary",
						"zone": "east"
					}
				}
			}
		}
	}]
}
